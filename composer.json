{
  "name": "laravel/lumen",
  "description": "The Laravel Lumen Framework.",
  "keywords": [
    "framework",
    "laravel",
    "lumen"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "barryvdh/laravel-cors": "2.0.3",
    "barryvdh/laravel-dompdf": "^0.9.0",
    "barryvdh/laravel-snappy": "^0.4.8",
    "box/spout": "^2.7",
    "darkaonline/swagger-lume": "8.",
    "dingo/api": "3.0.4",
    "fruitcake/laravel-cors": "^2.0",
    "guzzlehttp/guzzle": "^6.3",
    "illuminate/cache": "^8.0",
    "illuminate/container": "^8.0",
    "illuminate/mail": "^8.0",
    "illuminate/redis": "8.11.2",
    "illuminate/support": "^8.0",
    "intervention/image": "^2.4",
    "kreait/firebase-php": "^4.17",
    "laravel/helpers": "^1.3",
    "laravel/legacy-factories": "^1.0",
    "laravel/lumen-framework": "^8.0",
    "league/flysystem-aws-s3-v3": "~1.0",
    "league/glide": "^1.6",
    "league/glide-laravel": "^1.0",
    "maatwebsite/excel": "^3.1",
    "matthewbdaly/laravel-sms": "^1.0",
    "milon/barcode": "^8.0",
    "mpdf/mpdf": "^8.0",
    "palanik/lumen-cors": "dev-master",
    "predis/predis": "^1.1",
    "simplesoftwareio/simple-qrcode": "^2.0",
    "tecnickcom/tcpdf": "^6.2",
    "tymon/jwt-auth": "1.0.*",
    "vlucas/phpdotenv": "^5.2",
    "wisembly/elephant.io": "^3.3",
    "zircote/swagger-php": "^3.2",
    "ext-soap": "*"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "kitloong/laravel-migrations-generator": "^4.5",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "soapclient",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/Supports/helpers.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
